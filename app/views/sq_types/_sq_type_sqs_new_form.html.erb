<!-- Form sq start -->
<%= form_with(model: @sq, local: true) do |f| %>
<!-- Form sq end -->

  <!-- Label and input for sqtype_id start -->
  <!-- Input for sqtype_id start -->
  <%= f.hidden_field :sqtype_id, value: @sq_type.id %>
  <!-- Input for sqtype_id end -->
  <!-- Label and input for sqtype_id end -->

  <!-- Label and input for name start -->
  <div class="form-group">
    <%= f.label :name %>

    <!-- Input for name start -->
    <%= f.text_field :name, class: "form-control" %>
    <!-- Input for name end -->
  </div>
  <!-- Label and input for name end -->

  <!-- Label and input for pic start -->
  <div class="form-group">
    <%= f.label :pic %>

    <!-- Input for pic start -->
    <%= f.text_field :pic, class: "form-control" %>
    <!-- Input for pic end -->
  </div>
  <!-- Label and input for pic end -->

  <!-- Label and input for description start -->
  <div class="form-group">
    <%= f.label :description %>

    <!-- Input for description start -->
    <%= f.text_field :description, class: "form-control" %>
    <!-- Input for description end -->
  </div>
  <!-- Label and input for description end -->

  <%= f.submit "Create sq", class: "btn btn-block btn-outline-secondary" %>
<% end %>
